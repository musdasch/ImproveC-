#include <iostream>
using namespace std;


int main(){

	float c;
	cout << "Celsius: ";
	cin >> c;
	cout << "Celsius: " << c << " is Fahrenheit: " <<  ( ( c  * 1.8 ) + 32 ) << endl;

	return 0;
}
